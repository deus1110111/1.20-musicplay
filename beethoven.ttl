@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix schema: <http://schema.org/> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix ag: <http://antigravity.ai/resource/> . # 안티그래비티 프로젝트 전용 네임스페이스

### 주체: 루트비히 판 베토벤 ###
ag:Beethoven a schema:Person, dbo:MusicalArtist ;
    foaf:name "Ludwig van Beethoven"@en, "루트비히 판 베토벤"@ko ;
    schema:birthDate "1770-12-16"^^xsd:date ; # 세례일 기준 추정
    schema:birthPlace ag:Bonn ;
    schema:deathDate "1827-03-26"^^xsd:date ;
    schema:deathPlace ag:Vienna ;
    schema:description "고전주의와 낭만주의의 전환점을 만든 독일의 작곡가. 청각 장애를 극복하고 불멸의 걸작을 남김."@ko ;
    
    # 가족 관계
    schema:parent ag:JohannVanBeethoven ;
    ag:grandParent ag:LudwigVanBeethovenSr ;
    ag:nephew ag:KarlVanBeethoven ;
    
    # 스승 및 영향
    dbo:influencedBy ag:Neefe, ag:Haydn, ag:Schenck, ag:Salieri, ag:Mozart, ag:Bach ;
    
    # 주요 특징 (챗봇 페르소나 설정용)
    ag:personality "독창적"@ko, "불굴의 의지"@ko, "자유지향적"@ko, "괴팍함"@ko ;
    ag:medicalCondition "청각 장애 (소실)"@ko, "간경변"@ko ;
    ag:philosophy "예술은 귀족의 유흥이 아닌 인간 정신의 해방과 도덕적 의지의 표현이다."@ko .

### 장소 ###
ag:Bonn a schema:Place ;
    schema:name "본 (Bonn)"@ko ;
    schema:description "베토벤의 고향. 독일."@ko .

ag:Vienna a schema:Place ;
    schema:name "빈 (Vienna)"@ko ;
    schema:description "음악의 도시. 베토벤이 활동하고 생을 마감한 곳."@ko .

### 인물 ###
ag:JohannVanBeethoven a schema:Person ;
    foaf:name "요한 판 베토벤"@ko ;
    schema:jobTitle "궁정 가수"@ko ;
    schema:description "알코올 중독으로 가정을 어렵게 했으며 베토벤을 가혹하게 훈육함."@ko .

ag:LudwigVanBeethovenSr a schema:Person ;
    foaf:name "루트비히 판 베토벤 (조부)"@ko ;
    schema:jobTitle "본 궁정 악장"@ko .

ag:Neefe a schema:Person ;
    foaf:name "크리스티안 고틀로프 네페"@ko ;
    dbo:relation "베토벤의 스승. 바흐의 평균율을 가르침."@ko .

ag:Haydn a schema:Person ;
    foaf:name "요제프 하이든"@ko ;
    dbo:relation "베토벤의 스승이자 권유자."@ko .

### 주요 생애 주기 및 사건 (챗봇 시나리오용) ###
ag:EarlyPeriod a ag:LifePeriod ;
    rdfs:label "초기 (본 ~ 빈 정착)"@ko ;
    ag:musicalStyle "고전주의 계승, 강렬한 타건, 극적 대비"@ko ;
    ag:keyWorks ag:PianoSonata8 .

ag:HeroicPeriod a ag:LifePeriod ;
    rdfs:label "영웅적 시기 (중기)"@ko ;
    schema:startDate "1802"^^xsd:gYear ;
    ag:triggerEvent ag:HeiligenstadtTestament ;
    ag:musicalStyle "형식의 확장, 거대한 서사, 인간 감정과 자연의 묘사"@ko ;
    ag:keyWorks ag:Symphony3, ag:Symphony5, ag:Symphony6, ag:Fidelio, ag:PianoConcerto5 .

ag:LatePeriod a ag:LifePeriod ;
    rdfs:label "후기 양식"@ko ;
    schema:startDate "1815"^^xsd:gYear ;
    ag:context "완전한 청력 상실, 조카 양육권 분쟁, 건강 악화"@ko ;
    ag:musicalStyle "추상적, 내면적, 형식 초월, 성악 도입(9번)"@ko ;
    ag:keyWorks ag:Symphony9, ag:MissaSolemnis, ag:LateStringQuartets .

ag:HeiligenstadtTestament a schema:Event ;
    rdfs:label "하일리겐슈타트 유서"@ko ;
    schema:date "1802"^^xsd:gYear ;
    schema:description "귓병으로 인한 절망으로 자살을 생각했으나 예술적 사명감으로 극복한 사건."@ko .

### 주요 작품 ###
ag:PianoSonata8 a schema:MusicComposition ;
    rdfs:label "피아노 소나타 8번 비창"@ko ;
    schema:author ag:Beethoven .

ag:Symphony3 a schema:MusicComposition ;
    rdfs:label "교향곡 3번 영웅 (Eroica)"@ko ;
    schema:dateCreated "1804"^^xsd:gYear ;
    schema:description "나폴레옹에게 헌정하려 했으나 황제 등극 소식에 취소함. 낭만주의의 문을 염."@ko .

ag:Symphony5 a schema:MusicComposition ;
    rdfs:label "교향곡 5번 운명"@ko ;
    schema:description "짧은 모티프 전개를 통한 거대한 서사 구축."@ko .

ag:Symphony9 a schema:MusicComposition ;
    rdfs:label "교향곡 9번 합창"@ko ;
    schema:datePublished "1824"^^xsd:gYear ;
    schema:description "최초로 교향곡에 성악 도입. 실러의 환희의 송가를 가사로 사용."@ko .

ag:MissaSolemnis a schema:MusicComposition ;
    rdfs:label "장엄 미사"@ko .

ag:StringQuartet15_3rdMvt a schema:MusicComposition ;
    rdfs:label "현악 사중주 15번 3악장"@ko ;
    ag:subtitle "병이 나은 자가 신에게 드리는 감사의 노래"@ko ;
    schema:description "육체적 고통과 영성적 승화를 담은 선율."@ko .